{
  "name": "express-amplify",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nodemon src/index.ts",
    "clean": "rm -rf dist/ && rm -rf node_modules/ && rm -rf .amplify-hosting",
    "start": "ts-node src/index.ts",
    "build": "tsc --project ./tsconfig.json",
    "serve": "node dist/index.js",
    "postbuild": "chmod +x bin/postbuild.sh && ./bin/postbuild.sh",
    "push": "git add . && git commit --amend -m \"chore: update\" && git push -u -f origin main"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "prisma": "^5.9.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/http-errors": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.17",
    "dotenv": "^16.4.4",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "twig": "^1.17.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": "18.x.x",
    "yarn": "1.x.x"
  }
}
